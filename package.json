{
  "scripts": {
    "doc-prepare": "kuzdoc framework:install --destination .doc/",
    "doc-dev": "kuzdoc repo:dev -b .doc/ -d /sdk/go/1/ -v 1",
    "doc-build": "kuzdoc repo:build -b .doc/ -d /sdk/go/1/ -v 1",
    "doc-upload": "kuzdoc repo:deploy -b .doc/ -d /sdk/go/1/ -v 1",
    "doc-cloudfront": "kuzdoc repo:cloudfront -d /sdk/go/1/",
    "doc-deploy": "npm run doc-upload && npm run doc-cloudfront",
    "doc-netlify": "npm run doc-prepare && kuzdoc repo:build -b .doc/ -d / -v 1"
  },
  "dependencies": {
    "kuzdoc": "^1.2.2"
  }
}
